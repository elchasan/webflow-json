ld?", "": "295", "NULL": "10.4000/transposition.5192", "New York": "http://journals.openedition.org/transposition/5192", "": "Hors-série", "NULL": "2", "NULL": "NULL", "NULL": "NULL", "https://academicworks.cuny.edu/gc_etds/4651/": "NULL", "NULL": "NULL", "654e6d5c3378dba04e89b937": "http://journals.openedition.org/transposition/5192", "music-pedagogy-in-venezuela-s-plains--making-space-in-alma-llanera": "NULL", "thesis": "661a6c7316badb8f06c3efbe", "#N/A": "did-music-cause-the-end-of-the-world", "NULL": "journal", "NULL": "article", "NULL": "https://drive.google.com/file/d/1Vh4ctXNevxJGH9tNaCGG9r9eW4PwI0JE/view?usp=drive_link", "": "NULL", "": "NULL", "": "NULL"}
